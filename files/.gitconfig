[user]
    name=John Myles White
    email=jmw@johnmyleswhite.com

[color]
    ui=true

[core]
    editor=subl -w
    excludesfile=/Users/johnmyleswhite/.gitignore_global

[push]
    default=tracking

[pull]
    default=current

[alias]
    a=add
    b=branch
    branches=branch --list
    c=commit -m
    co=checkout
    com=checkout master
    d=diff
    dc=diff --cached
    f=fetch
    l=log
    ll=log --pretty=format:"%C(yellow)%h%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate --numstat
    lp=log -p
    m=merge
    mom=merge origin master
    p=push
    pom=push origin master
    s=status
    stashes=stash list
    undo=reset --soft HEAD^
